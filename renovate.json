{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
      {
        "matchUpdateTypes": [ "minor", "patch", "pin", "digest" ],
        "automerge": true,
        "automergeStrategy": "squash"
      }
    ],
    "rangeStrategy": "auto",
    "extends": [
      "config:base",
      ":preserveSemverRanges"
    ]
  }
